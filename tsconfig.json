{
  "compilerOptions": {
    "baseUrl": "./packages",
    "paths": {
      "@graphprotocol/*": ["./*/src"]
    },
    "skipLibCheck": true,
    "sourceMap": false,
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "Node",
    "useUnknownInCatchVariables": false,
    "allowSyntheticDefaultImports": true
  }
}
